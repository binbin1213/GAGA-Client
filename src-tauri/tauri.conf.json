{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "GAGA Client",
  "version": "0.1.0",
  "identifier": "com.gaga.client",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "GAGA Client",
        "label": "main",
        "width": 600,
        "height": 700,
        "minWidth": 500,
        "minHeight": 600,
        "maxWidth": 1920,
        "maxHeight": 1080,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "decorations": false,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "maximizable": true,
        "minimizable": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
